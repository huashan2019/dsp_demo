/* components/SliderDialog/SliderDialog.wxss */
@import "../../common_button.wxss";
.mask{
  width:100%;
  height:100%;
  position:fixed;
  background-color:#999;
  z-index:11;
  top:0;
  left:0;
  opacity:0.5;
}
.dialog-layout{
  width: 700rpx;
  height: 350rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -100rpx;
  margin-left: -350rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  z-index: 12;
}
.layout-land-delay{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center; 
}
.layout-land-delay button{
  width: 160rpx;
  height: 64rpx;
  font-size: 35rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin: 0;
}
.layout-land-delay image{
  width: 60rpx;
  height: 60rpx;
}